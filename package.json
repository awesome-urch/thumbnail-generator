{
  "name": "thumbnail-generator",
  "version": "0.0.6",
  "description": "Generate thumbnails just by describing it",
  "scripts": {
    "start:gulp": "npm-run-all gulp start",
    "gulp": "gulp",
    "serve": "node dist/src/server/start.js",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node' server.ts",
    "start": "npm run serve",
    "test": "mocha -r ts-node/register ./test/* ",
    "testdebug": "mocha --inspect-brk --colors --recursive --require ts-node/register --watch-extensions ts \"**/*spec.ts\" --exclude \"node_modules/**\"",
    "db:migrate": "npx knex migrate:latest",
    "db:seed": "npx knex seed:run",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint copy-assets",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.6",
    "bcrypt": "^5.0.0",
    "chai": "^4.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "4.17.1",
    "express-session": "^1.17.3",
    "knex": "0.20.8",
    "mocha": "^10.2.0",
    "mysql": "^2.18.1",
    "oauth2-server": "^3.1.1",
    "openai": "^3.1.0",
    "pg": "7.18.1",
    "sinon": "^15.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.5",
    "@types/mocha": "^10.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.18",
    "@types/oauth2-server": "^3.0.13",
    "@types/pg": "^8.6.6",
    "@types/shelljs": "^0.8.11",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "del": "^7.0.0",
    "eslint": "^8.31.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.20",
    "shelljs": "^0.8.5",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
